{
  "global": {
    "_comment": "Because of incompatibility with ffb and reshade, for fullscreen i use xenos to inject the ffb dll post launch switch to windowed if you have issue, also in 4K i added some magnifier in reshade to move some ui element",
    "windowed": "General,Windowed,1",
    "originalRes": "1024x768",
    "showGameOptionBezel": "False",
    "showGameOptionCrt": "False",
    "showGameOptionVsync": "False",
    "showGameOptionFFB": "True",
    "showGameOptionKeepAspectRatio": "False",
    "upscaleFullscreen": "True",
    "upscaleWindowed": "True",
    "magpieNoLaunchIfSameRes": "True",
    "need60hz": "True",
    "magpieClass": "FREEGLUT",
    "magpie3DGameMode": "True",
    "magpieDisableDirectFlip": "False"
  },
  "tpoptions": {
    "set_displaymode_recommanded": {
      "General||Windowed": "1"
    },
    "set_fullscreen": {
      "General||Windowed": "0"
    },
    "set_windowed": {
      "General||Windowed": "1"
    },
    "ffb": {
      "General||Enable Outputs": "1"
    },
    "recommanded_tp_settings & xinput": {
      "General||Reverse Y Axis": "1"
    }
  },
  "gameoptions": {
    "recommanded_gameoptions": {
      "runRivaTuner": "True",
      "magpieDelay": "3"
    },
    "recommanded_gameoptions & ffb & optional_reshade": {
      "useInjector": "true",
      "injectorDllList": "winmm.dll.inject",
      "injectorDelay": "5",
      "injectorWaitForGameWindow": "True"
    }
  }
}