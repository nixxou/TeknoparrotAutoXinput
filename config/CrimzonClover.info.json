{
  "global": {
    "comment": "For fullscreen on my amd gpu, i had to create a custom res of 1280x768 60hz",
    "windowed": "General,DisplayMode,Windowed",
    "originalRes": "1280x768",
    "showGameOptionBezel": "False",
    "showGameOptionCrt": "True",
    "showGameOptionVsync": "True",
    "showGameOptionFFB": "False",
    "showGameOptionKeepAspectRatio": "True",
    "showGameOptionPatchLang": "False",
    "showGameOptionPerf": "True",
    "disableCrtForLowPerf": "False",
    "upscaleFullscreen": "True",
    "upscaleWindowed": "False",
    "forceResSwitchFullscreen": "True",
    "canUseJoyInsteadOfDpad": "True",
    "magpieClass": "CrimzonClover for NESiCAxLive",
    "magpie3DGameMode": "False",
    "magpieDisableDirectFlip": "False",
    "magpieAllowScalingMaximized": "False",
    "magpieLaunchBefore": "False",
    "magpieNoMoveWindow": "False"
  },
  "tpoptions": {
    "set_displaymode_recommanded": {
      "General||DisplayMode": "Windowed"
    },
    "set_windowed": {
      "General||DisplayMode": "Windowed"
    },
    "set_fullscreen": {
      "General||DisplayMode": "Fullscreen"
    },
    "replace_gameid": {
      "General||Player Name for new cards (P1)": "{{customname}}"
    }
  },
  "gameoptions": {
    "recommanded_gameoptions & crtfilter": {
      "magpieScaling": "6"
    },
    "recommanded_gameoptions & fullscreen & set_resolution & optional_reshade": {
      "useInjector": "true",
      "injectorDllList": "dxgi.dll.inject",
      "injectorDelay": "0"
    }
  }
}