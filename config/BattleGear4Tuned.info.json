{
	"global": {
		"windowed": "General,Windowed,1",
		"magpieClass": "OpenParrot - Battle Gear 4 Tuned",
		"magpie3DGameMode": "True",
		"magpieDisableDirectFlip": "False",
		"magpieAllowScalingMaximized": "False",
		"magpieLaunchBefore": "False"
	},
	"tpoptions": {	
        "set_displaymode_in_tp_settings & set_displaymode_recommanded": {
            "General||Windowed": "0"
        },
        "set_displaymode_in_tp_settings & set_fullscreen": {
            "General||Windowed": "0"
        },
        "set_displaymode_in_tp_settings & set_windowed": {
            "General||Windowed": "1"
        },
        "set_res_in_tp_settings & 720p": {
            "General||Custom Resolution (Professional Edition)": "1",
            "General||Custom Resolution Width": "1280",
            "General||Custom Resolution Height": "720"
        },
        "set_res_in_tp_settings & 1080p": {
            "General||Custom Resolution (Professional Edition)": "1",
            "General||Custom Resolution Width": "1920",
            "General||Custom Resolution Height": "1080"
        },
        "set_res_in_tp_settings & 2k": {
            "General||Custom Resolution (Professional Edition)": "1",
            "General||Custom Resolution Width": "2560",
            "General||Custom Resolution Height": "1440"
        },
        "set_res_in_tp_settings & 4k": {
            "General||Custom Resolution (Professional Edition)": "1",
            "General||Custom Resolution Width": "3840",
            "General||Custom Resolution Height": "2160"
        },		
		"recommanded_tp_settings": {
			"General||IntroFix": "1",
			"General||Enable All Tracks": "1",
			"General||Enable Outputs": "1",
			"General||Professional Edition Enable": "1"
		},
		"shifter": {
			"General||Professional Edition Hold Gear": "1"
		},
		"!shifter": {
			"General||Professional Edition Hold Gear": "0"
		},		
		"replace_network": {
			"Network||BroadcastAddress": "{{broadcastaddress}}"
		}
	},
	"gameoptions": {}
}