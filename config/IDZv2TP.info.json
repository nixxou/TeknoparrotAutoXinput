{
	"global": {
		"windowed": "Graphics,DisplayMode,Windowed",
		"magpieClass": "Direct3DWindowClass",
		"magpie3DGameMode": "True",
		"magpieDisableDirectFlip": "False",
		"magpieAllowScalingMaximized": "False",
		"magpieLaunchBefore": "False",
		"magpieNoMoveWindow": "False",
		"magpieNoLateFocus": "False"
	},
	"tpoptions": {
		"set_displaymode_in_tp_settings & set_displaymode_recommanded": {
            "General||DisplayMode": "Fullscreen"
        },			
        "set_displaymode_in_tp_settings & set_fullscreen": {
            "General||DisplayMode": "Fullscreen"
        },
        "set_displaymode_in_tp_settings & set_windowed": {
            "General||DisplayMode": "Windowed"
        },
        "set_res_in_tp_settings & 720p": {
			"General||Screen Width": "1280",
            "General||Screen Height": "720",
            "General||Render Resolution X": "1280",
			"General||Render Resolution Y": "720"
        },
        "set_res_in_tp_settings & 1080p": {
			"General||Screen Width": "1920",
            "General||Screen Height": "1080",
            "General||Render Resolution X": "1920",
			"General||Render Resolution Y": "1080"
        },
        "set_res_in_tp_settings & 2k": {
			"General||Screen Width": "2560",
            "General||Screen Height": "1440",
            "General||Render Resolution X": "2560",
			"General||Render Resolution Y": "1440"
        },
        "set_res_in_tp_settings & 4k": {
			"General||Screen Width": "3840",
            "General||Screen Height": "2160",
            "General||Render Resolution X": "3840",
			"General||Render Resolution Y": "2160"
        },		
		"replace_network": {
			"Network||Force Specific Network Adapter": "1",
			"Network||Network Adapter IP": "{{networkip}}"
		},
		"replace_gameid": {
			"General||OnlineID": "{{apm3id}}"
		}
	},
	"gameoptions": {}
}