{
  "global": {
    "_comment": "This one was tricky to make it work with dgvodoo for upscale",
    "windowed": "General,Windowed,1",
    "showGameOptionBezel": "False",
    "showGameOptionCrt": "False",
    "showGameOptionVsync": "True",
    "showGameOptionFFB": "False",
    "showGameOptionKeepAspectRatio": "False",
    "showGameOptionPatchLang": "False",
    "showGameOptionPerf": "High",
    "upscaleFullscreen": "True",
    "upscaleWindowed": "False",
    "rom": "og",
    "target": "ringwide",
    "64bits": "False",
    "magpieClass": "A",
    "magpie3DGameMode": "True",
    "magpieDisableDirectFlip": "False",
    "magpieAllowScalingMaximized": "True",
    "magpieLaunchBefore": "False",
    "magpieNoMoveWindow": "True",
    "magpieNoLateFocus": "False",
    "recommanded_vjoy_fullscreen": "sinden_and_notsinden",
    "recommanded_vjoy_windowed": "all"
  },
  "tpoptions": {
    "set_displaymode_recommanded": {
      "General||Windowed": "0"
    },
    "set_fullscreen": {
      "General||Windowed": "0"
    },
    "set_windowed": {
      "General||Windowed": "1"
    },
    "recommanded_tp_settings": {
      "General||Enable Outputs": "1",
      "General||HideCursor": "1"
    }
  },
  "gameoptions": {
    "recommanded_gameoptions": {
      "runRivaTuner": "True"
    },
    "recommanded_gameoptions & at_least_one_sinden & no_sinden_using_vjoy": {
      "gun_ExtraSinden": "-action \"set-offsets -1.5,1.03,-1.5,1.03\"",
      "gun_useExtraSinden": "True"
    },
    "recommanded_gameoptions & at_least_one_sinden": {
      "tmpGunXFormula": "[X]+(([X]-(32767/2))*0.03093)",
      "tmpGunYFormula": "[Y]+(([Y]-(32767/2))*0.03093)"
    }
  }
}