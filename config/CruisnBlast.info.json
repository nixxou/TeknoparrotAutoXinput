{
  "global": {
    "_comment": "120hz+ support fullscreen & windowed",
    "windowed": "General,Windowed,1",
    "originalRes": "1920x1080",
    "showGameOptionBezel": "False",
    "showGameOptionCrt": "False",
    "showGameOptionVsync": "False",
    "showGameOptionFFB": "True",
    "showGameOptionKeepAspectRatio": "False",
    "showGameOptionPatchLang": "False",
    "showGameOptionPerf": "True",
    "need60hzForLowPerf": "True",
    "upscaleFullscreen": "True",
    "upscaleWindowed": "True",
    "magpieNoLaunchIfSameRes": "True",
    "magpieClass": "SDL_app",
    "magpie3DGameMode": "True",
    "magpieDisableDirectFlip": "False",
    "magpieAllowScalingMaximized": "False",
    "magpieLaunchBefore": "False",
    "magpieNoMoveWindow": "False"
  },
  "tpoptions": {
    "set_displaymode_recommanded": {
      "General||Windowed": "0"
    },
    "set_fullscreen": {
      "General||Windowed": "0"
    },
    "set_windowed": {
      "General||Windowed": "1"
    },
    "set_resolution & 720p": {
      "General||Enable Custom Resolution": "1",
      "General||Custom Resolution": "1280x720"
    },
    "set_resolution & 1080p": {
      "General||Enable Custom Resolution": "1",
      "General||Custom Resolution": "1920x1080"
    },
    "set_resolution & 2k": {
      "General||Enable Custom Resolution": "1",
      "General||Custom Resolution": "2560x1440"
    },
    "set_resolution & 4k": {
      "General||Enable Custom Resolution": "1",
      "General||Custom Resolution": "3840x2160"
    },
    "game_native_res": {
      "General||Enable Custom Resolution": "1",
      "General||Custom Resolution": "1920x1080"
    },
    "ffb": {
      "General||Enable Outputs": "0",
      "General||Allow FFB Plugin": "1"
    },
    "recommanded_tp_settings": {
      "General||Enable Longer Timers": "1",
      "General||Enable Higher Res Shadows": "1"
    }
  },
  "gameoptions": {
    "60hz": {
      "runRivaTuner": "True"
    },
	"low_perfprofile": {
      "runRivaTuner": "True"	
	},
    "recommanded_gameoptions & optional_reshade": {
      "useInjector": "true",
      "injectorDllList": "Reshade32.dll",
      "injectorDelay": "0"
    }
  }
